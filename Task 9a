# Install the OpenAI package (run this in your terminal or command prompt)
# pip install openai

import openai

# Set your API key securely (avoid hardcoding in production)
openai.api_key = "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"

# Make a request to the ChatCompletion endpoint
response = openai.ChatCompletion.create(
    model="gpt-3.5-turbo",
    messages=[
        {"role": "user", "content": "Give me 3 ideas that I could build using OpenAI APIs"}
    ]
)

# Print the response content
print(response['choices'][0]['message']['content'])
